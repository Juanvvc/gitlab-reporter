{
  "name": "gitlab-reporter",
  "version": "2.2.4",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "docs": "node_modules/.bin/jsdoc -c .jsdoc.json -R README.md src",
    "deploy": "yarn build ; node_modules/.bin/push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@mdi/font": "^5.3.45",
    "axios": "^0.19.0",
    "basil.js": "^0.4.11",
    "frappe-gantt": "https://github.com/Juanvvc/gantt.git",
    "moment": "^2.26.0",
    "vue": "^2.6.11",
    "vue-simple-calendar": "^4.4.0",
    "vuetify": "2.4.5",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.1",
    "@vue/cli-plugin-eslint": "^4.4.1",
    "@vue/cli-service": "^4.4.1",
    "babel-eslint": "^10.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^7.2.0",
    "eslint-plugin-vue": "^7.1.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.5",
    "fibers": "^5.0.0",
    "jsdoc": "^3.6.4",
    "jsdoc-vuejs": "^3.0.5",
    "minami": "^1.2.3",
    "push-dir": "^0.4.1",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.4",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "vuetify"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "Console|[iI]gnored"
        }
      ],
      "vuetify/no-deprecated-classes": "error",
      "vuetify/grid-unknown-attributes": "error",
      "vuetify/no-legacy-grid": "error",
      "vue/valid-v-slot": ["error", {
          "allowModifiers": true
      }]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "license": "MIT",
  "author": "Juanvi Vera <juanvvc@gmail.com>",
  "description": "A task manager for gitlab"
}
